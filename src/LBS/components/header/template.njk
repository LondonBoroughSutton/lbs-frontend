        </div>

        <div class="lbs-header__nav-container">
            {% if params.search !== false %}
            <div class="lbs-header__mobile-icons">
                <button class="lbs-header__mobile-search-btn govuk-header__menu-button govuk-js-header-toggle"
                        id="lbs-header__mobile__search-btn" type="button" aria-controls="lbs-header__search"
                        aria-label="{{ params.searchButtonLabel | default('Show or hide search box') }}">
                    <span class="govuk-visually-hidden">Search</span>
                </button>
                {% endif %}
            {% if params.navigation %}
                <button class="lbs-header__mobile-menu-btn govuk-header__menu-button govuk-js-header-toggle"
                        id="lbs-header__mobile__menu-btn" type="button" aria-controls="lbs-header__navigation"
                        aria-label="{{ params.menuButtonLabel | default('Show or hide navigation menu') }}">
                    <span class="govuk-visually-hidden">Menu</span>
                    <svg class="lbs-header__mobile__burger-btn" viewBox="0 0 40 26"
                         xmlns="http://www.w3.org/2000/svg">
                        <rect class="lbs-header__mobile-menu-btn--1" width="40" height="4" rx="3" ry="3"/>
                        <rect class="lbs-header__mobile_burger-btn--2" width="40" height="4" y="10" rx="3" ry="3"/>
                        <rect class="lbs-header__mobile_burger-btn--3" width="40" height="4" y="20" rx="3" ry="3"/>
                    </svg>
                </button>
            {% endif %}
            </div>
            {% if params.navigation %}
                <nav role="navigation" id="lbs-header__navigation" class="lbs-header__navigation">
                    <ul class="lbs-header__navigation--level1"
                        aria-label="{{ params.navigationLabel | default('Navigation menu') }}">
                        {% for item in params.navigation %}
                            <li>
                                <a href="{{ item.href }}"
                                   class="lbs-header__navigation__item--level1">{{ item.text }}</a>
                                {% if item.subItems.length > 0 %}
                                    <ul class="lbs-header__navigation--level2">
                                        {% for subItem in item.subItems %}
                                            <li>
                                                <a href="/{{ subItem.href }}/">{{ subItem.text }}</a>
                                            </li>
                                        {% endfor %}
                                    </ul>
                                {% endif %}
                            </li>
                        {% endfor %}
                    </ul>
                </nav>
            {% endif %}
        </div>
        {% if params.search !== false %}
        <div class="lbs-header__search" id="lbs-header__search">
            <form id="lbs-header__search-form" action="/" method="post" autocomplete="off">
                <input type="text" name="search" class="input" id="lbs__searchBox" placeholder="Search">
                <button type="submit" class="search" id="search-btn"></button>
            </form>
            <button class="lbs_header__search__close-btn govuk-header__menu-button govuk-js-header-toggle"
                    aria-controls="lbs-header__search-block" id="lbs_header__search__close-btn">
                Close
            </button>
        </div>
        {% endif %}
    </div>
</header>